    "@tanstack/solid-query-persist-client": "^5.55.4",
    "@thinke/toast": "^1.2.2",
    "dayjs": "^1.11.13",
    "js-base64": "^3.7.7",
    "js-cookie": "^3.0.5",
    "ky": "^1.7.2",
    "lodash": "npm:lodash-es@^4.17.21",
    "solid-js": "^1.8.22"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.5.1",
    "@ssrx/vite": "^0.6.1",
