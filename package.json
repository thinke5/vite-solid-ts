    "eslint-plugin-format": "^0.1.2",
    "less": "^4.2.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.6.2",
    "unocss": "^0.62.3",
    "unplugin-inject-preload": "^2.0.5",
    "vite": "^5.4.3",
    "vite-plugin-singlefile": "^2.0.2",
    "vite-plugin-solid": "^2.10.2",
    "vite-tsconfig-paths": "^5.0.1"
